        xmlns:local="clr-namespace:_Project"
        mc:Ignorable="d"
        Title="Student_Problem_List" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="Student_ProblemList" HorizontalAlignment="Left" Margin="186,187,0,0" VerticalAlignment="Top" Width="120"/>
